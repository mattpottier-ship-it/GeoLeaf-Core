{
    "name":  "geoleaf",
    "version":  "4.0.0",
    "description":  "GeoLeaf â€” Modular JavaScript mapping library built on Leaflet. v4.0: Full ESM migration, tree-shaking, selective imports. Features: Security (XSS protection), Performance (optimized build pipeline), Typed Errors, Validators, and comprehensive TypeScript support.",
    "type":  "module",
    "main":  "dist/geoleaf.umd.js",
    "module":  "dist/geoleaf.esm.js",
    "unpkg":  "dist/geoleaf.min.js",
    "jsdelivr":  "dist/geoleaf.min.js",
    "types":  "index.d.ts",
    "browser":  "dist/geoleaf.umd.js",
    "exports":  {
                    ".":  {
                              "import":  "./dist/geoleaf.esm.js",
                              "require":  "./dist/geoleaf.umd.js",
                              "types":  "./index.d.ts"
                          }
                },
    "sideEffects":  [
                        "src/modules/globals.js",
                        "src/modules/globals.core.js",
                        "src/modules/globals.config.js",
                        "src/modules/globals.geojson.js",
                        "src/modules/globals.ui.js",
                        "src/modules/globals.poi.js",
                        "src/modules/globals.api.js",
                        "src/modules/geoleaf.api.js",
                        "src/modules/config/geoleaf-config/*.js",
                        "src/app/helpers.js",
                        "src/app/init.js",
                        "src/app/boot.js",
                        "src/app/init-feature-modules.js",
                        "src/lazy/*.js",
                        "src/bundle-entry.js",
                        "src/bundle-esm-entry.js",
                        "**/*.css"
                    ],
    "scripts":  {
                    "build":  "rollup -c",
                    "build:watch":  "rollup -c -w",
                    "clean":  "rimraf dist \u0026\u0026 mkdir dist",
                    "test":  "node scripts/smoke-test.cjs",
                    "test:all":  "npm run test \u0026\u0026 npm run test:jest",
                    "test:jest":  "jest --config jest.config.cjs",
                    "test:esm":  "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.cjs --testPathIgnorePatterns=/node_modules/ /dist/ --setupFilesAfterEnv ./__tests__/setup-esm.js --testPathPattern=\\.esm\\.test\\.js$",
                    "test:bundle":  "jest __tests__/bundle.test.js --config jest.config.cjs --no-coverage",
                    "test:watch":  "jest --config jest.config.cjs --watch",
                    "test:coverage":  "jest --config jest.config.cjs --coverage --coverageDirectory=coverage",
                    "test:ci":  "npm run test:coverage \u0026\u0026 npm run test",
                    "benchmark":  "node scripts/benchmark.cjs",
                    "benchmark:baseline":  "node scripts/benchmark.cjs --baseline",
                    "benchmark:compare":  "node scripts/benchmark.cjs --compare",
                    "benchmark:json":  "node scripts/benchmark.cjs --json",
                    "benchmark:ci":  "node scripts/benchmark.cjs --ci",
                    "lint":  "eslint src/**/*.js __tests__/**/*.js",
                    "lint:fix":  "eslint src/**/*.js __tests__/**/*.js --fix",
                    "format":  "prettier --write \"src/**/*.js\" \"__tests__/**/*.js\" \"*.{json,md}\"",
                    "format:check":  "prettier --check \"src/**/*.js\" \"__tests__/**/*.js\" \"*.{json,md}\"",
                    "build:css":  "postcss src/css/geoleaf-main.css -o dist/geoleaf-main.min.css --map",
                    "build:all":  "npm run clean \u0026\u0026 npm run build \u0026\u0026 npm run build:css",
                    "verify":  "npm run build:all \u0026\u0026 npm run test:bundle",
                    "build:deploy":  "node scripts/build-deploy.cjs",
                    "build:deploy:storage":  "node scripts/build-deploy.cjs --plugins=storage",
                    "build:deploy:addpoi":  "node scripts/build-deploy.cjs --plugins=addpoi",
                    "build:deploy:all":  "node scripts/build-deploy.cjs --plugins=all",
                    "build:deploy:full":  "node scripts/build-deploy.cjs --full",
                    "sync:public:dry":  "powershell -ExecutionPolicy Bypass -File scripts/sync-to-public.ps1",
                    "sync:public":  "powershell -ExecutionPolicy Bypass -File scripts/sync-to-public.ps1 -Confirm",
                    "sync:public:clean":  "powershell -ExecutionPolicy Bypass -File scripts/sync-to-public.ps1 -Confirm -Clean",
                    "sync:public:push":  "powershell -ExecutionPolicy Bypass -File scripts/sync-to-public.ps1 -Confirm -Push",
                    "dist:plugins":  "node scripts/dist-plugins.cjs",
                    "publish:plugins":  "npm run build \u0026\u0026 npm run dist:plugins \u0026\u0026 node scripts/publish-plugins.cjs",
                    "prepublishOnly":  "npm audit --audit-level=high \u0026\u0026 npm run lint \u0026\u0026 npm run build:all \u0026\u0026 npm run test:all",
                    "prepare":  "husky"
                },
    "lint-staged":  {
                        "src/**/*.js":  [
                                            "eslint --fix",
                                            "prettier --write"
                                        ],
                        "__tests__/**/*.js":  [
                                                  "eslint --fix",
                                                  "prettier --write"
                                              ],
                        "*.{json,md}":  [
                                            "prettier --write"
                                        ]
                    },
    "peerDependencies":  {
                             "@maplibre/maplibre-gl-leaflet":  "^0.1.3",
                             "leaflet":  "^1.9.4",
                             "leaflet.markercluster":  "^1.5.3",
                             "leaflet.vectorgrid":  "^1.3.0",
                             "maplibre-gl":  "^5.0.0"
                         },
    "peerDependenciesMeta":  {
                                 "leaflet.vectorgrid":  {
                                                            "optional":  true
                                                        },
                                 "maplibre-gl":  {
                                                     "optional":  true
                                                 },
                                 "@maplibre/maplibre-gl-leaflet":  {
                                                                       "optional":  true
                                                                   }
                             },
    "devDependencies":  {
                            "@rollup/plugin-alias":  "^6.0.0",
                            "@rollup/plugin-commonjs":  "^29.0.0",
                            "@rollup/plugin-node-resolve":  "^16.0.3",
                            "@rollup/plugin-replace":  "^6.0.3",
                            "@rollup/plugin-terser":  "^0.4.4",
                            "@testing-library/dom":  "^10.4.0",
                            "@testing-library/jest-dom":  "^6.4.2",
                            "@types/jest":  "^29.5.12",
                            "cssnano":  "^7.1.2",
                            "eslint":  "^8.57.0",
                            "eslint-config-prettier":  "^9.1.0",
                            "eslint-plugin-security":  "^3.0.1",
                            "husky":  "^9.0.11",
                            "jest":  "^29.7.0",
                            "jest-environment-jsdom":  "^29.7.0",
                            "lint-staged":  "^15.2.2",
                            "madge":  "^8.0.0",
                            "postcss":  "^8.5.6",
                            "postcss-cli":  "^11.0.1",
                            "postcss-import":  "^16.1.1",
                            "prettier":  "^3.2.5",
                            "rimraf":  "^5.0.5",
                            "rollup":  "^4.12.0",
                            "rollup-plugin-filesize":  "^10.0.0",
                            "rollup-plugin-visualizer":  "^6.0.5"
                        },
    "files":  [
                  "dist/",
                  "index.d.ts",
                  "README.md",
                  "docs/"
              ],
    "keywords":  [
                     "leaflet",
                     "map",
                     "mapping",
                     "gis",
                     "geospatial",
                     "frontend",
                     "geoleaf",
                     "geojson",
                     "gpx",
                     "security",
                     "xss-protection",
                     "typescript",
                     "performance",
                     "debounce",
                     "throttle"
                 ],
    "repository":  {
                       "type":  "git",
                       "url":  "https://github.com/mattpottier-ship-it/GeoLeaf-Core.git"
                   },
    "bugs":  {
                 "url":  "https://github.com/mattpottier-ship-it/GeoLeaf-Core/issues"
             },
    "homepage":  "https://github.com/mattpottier-ship-it/GeoLeaf-Core#readme",
    "author":  "GeoLeaf Project",
    "license":  "MIT"
}